 <div class="row sign-up-form">
     <div class="col s6 m6 offset-s4">
       <div class="card blue-grey darken-1">
         <div class="card-content white-text">
           <span class="card-title">Name: <%=@course.short_name %></span>
           <p>Email: <%=@course.name %></p>
           <p>Course Students Enrolled:
            <% if @course.students.empty? %>
               None 
            <% else %>
              <%=pluralize(@course.students.count, "student") %>
            <% end %>
           </p>
           <!--<a class="waves-effect waves-light btn">-->
           <!--<i class="material-icons right">create</i>Edit</a>-->
        </div>
     </div>
 </div>

  